				</div>
			</div>
			<div class="s5-cards">
				<div>
					<span class="icon">{{ row.content.field_icon3[0] }}</span>
				</div>
				<h4>
					{{ row.content.field_number_text3[0] }}
				</h4>
				<div class="pb-5">
					{{ row.content.field_text3[0] }}
				</div>
			
			<div>
				<a class="read-more-btn" href={{row.content.field_btn3[0]['#url'] }}>{{row.content.field_btn3[0]['#title'] }}</a>
			</div>
		</div>
	</div>


</div>{% endfor %}</section>
